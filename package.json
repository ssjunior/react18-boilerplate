{
  "name": "React 18 boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "i18next": "^21.8.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.17.2",
    "react-redux": "^8.0.2",
    "react-router-dom": "6",
    "theme-ui": "^0.14.6"
  },
  "scripts": {
    "start": "webpack serve --config ./webpack/webpack.dev.js --mode development",
